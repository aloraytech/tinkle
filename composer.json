{
    "name": "tinkle/tinkle",
    "type": "project",
    "description": "The Tinkle Framework.",
    "keywords": ["framework", "tinkle"],
    "license": "MIT",

    "authors": [
        {
            "name": "Krishanu Bhattacharya",
            "email": "krishanu.info@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "config/",
            "Tinkle\\": "src/",
            "Database\\": "database/",
            "Plugins\\": "app/Plugins/",
            "Themes\\": "resources/themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "require": {
        "php": "8.0",
        "vlucas/phpdotenv": "^5.3",
        "symfony/http-foundation": "^5.3",
        "twig/twig": "^3.3",
      "ext-password": "*",
      "ext-json": "*",
      "ext-gd": "*"
    },
    "minimum-stability": "dev"
}
