{
    "name": "tinkle/tinkle",
    "type": "project",
    "description": "The Tinkle Framework.",
    "keywords": ["framework", "tinkle"],
    "license": "(MIT or GPL-3.0-or-later)",
    "authors": [
        {
            "name": "Krishanu Bhattacharya",
            "email": "krishanu.info@gmail.com",
            "homepage": "https://aloraytech.com/tinkle",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "config/",
            "Tinkle\\": "src/",
            "Database\\": "database/",
            "Plugins\\": "app/Plugins/",
            "Themes\\": "resources/themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "require": {
        "php": "^8.0",
        "vlucas/phpdotenv": "^5.3",
        "symfony/http-foundation": "^5.3",
        "twig/twig": "^3.3",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-pdo": "*",
        "phpoffice/phpspreadsheet": "dev-master",
        "razorpay/razorpay": "2.*",
        "illuminate/database": "^8.63",
        "illuminate/events": "^8.63"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
